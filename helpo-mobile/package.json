      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./setupTests.js"
    ],
    "transform": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    }
  },
  "dependencies": {
    "expo": "^27.0.1",
    "react": "16.3.1",
    "react-native": "~0.55.2",
    "react-router-native": "4.2.0",
    "react-native-elements": "0.19.1",
    "react-native-google-place-picker": "1.2.1"
  }
